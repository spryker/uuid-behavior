{
  "name": "spryker/uuid-behavior",
  "type": "propel-behavior",
  "description": "UuidBehavior module",
  "license": "proprietary",
  "require": {
    "php": ">=7.1",
    "spryker/kernel": "^3.0.0",
    "spryker/propel-orm": "^1.6.0",
    "spryker/zend": "^2.1.0"
  },
  "require-dev": {
    "spryker/code-sniffer": "*",
    "spryker/testify": "*"
  },
  "support": {
    "source": "https://github.com/spryker/uuid-behavior"
  },
  "autoload": {
    "psr-4": {
      "Spryker\\": "src/Spryker/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "name": "uuid",
    "class": "\\Spryker\\Zed\\UuidBehavior\\Persistence\\Propel\\Behavior\\UuidBehavior",
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "scripts": {
    "cs-check": "phpcs --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml -v --ignore=/uuid-behavior/vendor/,/templates/ ./",
    "cs-fix": "phpcbf --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml -v --ignore=/uuid-behavior/vendor/,/templates/ ./"
  },
  "config": {
    "sort-packages": true
  }
}
