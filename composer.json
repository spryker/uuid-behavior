{
  "name": "spryker/uuid-behavior",
  "type": "propel-behavior",
  "description": "UuidBehavior module",
  "license": "proprietary",
  "require": {
    "php": ">=7.3",
    "spryker/kernel": "^3.30.0",
    "spryker/laminas": "^1.0.0",
    "spryker/propel-orm": "^1.6.0",
    "spryker/util-uuid-generator": "^1.0.0"
  },
  "require-dev": {
    "phpstan/phpstan": "*",
    "spryker/code-sniffer": "*",
    "spryker/propel": "*",
    "spryker/testify": "*"
  },
  "autoload": {
    "psr-4": {
      "Spryker\\": "src/Spryker/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SprykerTest\\": "tests/SprykerTest/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/ tests/",
    "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/ tests/",
    "phpstan": "phpstan analyse -c phpstan.neon -l 5 src/",
    "phpstan-setup": "cp composer.json composer.backup && COMPOSER_MEMORY_LIMIT=-1 composer require --dev phpstan/phpstan-shim:^0.11 && mv composer.backup composer.json"
  },
  "extra": {
    "name": "uuid",
    "class": "\\Spryker\\Zed\\UuidBehavior\\Persistence\\Propel\\Behavior\\UuidBehavior",
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  },
  "support": {
    "source": "https://github.com/spryker/uuid-behavior"
  }
}
